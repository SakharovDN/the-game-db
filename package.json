    "i18next-icu": "^2.3.0",
    "i18next-resources-for-ts": "^1.5.0",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^14.1.1",
    "react-redux": "^9.1.2",
